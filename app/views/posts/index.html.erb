<h1>All Posts</h1>
<div>
  <%= link_to "Create New Post", "/posts/new", class: 'btn btn-primary' %>
</div>
<% @posts.each do |post| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
          <%= link_to post.title, post %>
      </h4>
    </div>
  </div>
<% end %>
  